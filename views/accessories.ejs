<%- include('common/header.ejs') %>




  <body>

    <div id="carouselExampleSlidesOnly" class="carousel-fade" data-bs-ride="carousel">
      <div class="carousel-inner">
        <% banners.forEach((banner, index) => { %>
          <div class="carousel-item <%= index === 0 ? 'active' : '' %>" data-bs-interval="2000">
            <img src="/static/images/category/productbanner/<%= banner.product_banner %>" class="d-block w-100" alt="<%= banner.product_title %>">
          </div>
        <% }); %>
      </div>
    </div>
    
    <div class="container acc_tabs text-center my-5 py-1">
      <div class="nav nav-tabs" id="nav-tab" role="tablist">
        <!-- Keyboards Tab -->
        <div class="col-4 p-5 active" id="nav-home-tab" data-bs-toggle="tab" data-bs-target="#nav-home" role="tab" aria-controls="nav-home" aria-selected="true">
          <div class="card bg-transparent border-0">
            <div class="card-body">
              <div id="keyboardCarousel" class="carousel-fade" data-bs-ride="carousel">
                <div class="carousel-inner">
                  <% keyboard.forEach((keyboard, index) => { %>
                    <% var keyboardImages = keyboard.product_image.split(','); %>
                    <% keyboardImages.forEach((image, imgIndex) => { %>
                      <div class="carousel-item <%= imgIndex === 0 && index === 0 ? 'active' : '' %>" data-bs-interval="2000">
                        <img src="/static/images/category/product/<%= image.trim() %>" alt="<%= keyboard.product_title %>" class="img-fluid d-block w-100">
                      </div>
                    <% }); %>
                  <% }); %>
                </div>
              </div>
              <div class="card-footer h6 fw-bold text-light mt-3">KEYBOARD</div>
            </div>
          </div>
        </div>
        
        <!-- Mice Tab -->
        <div class="col-4 p-5" id="nav-profile-tab" data-bs-toggle="tab" data-bs-target="#nav-profile" role="tab" aria-controls="nav-profile" aria-selected="false">
          <div class="card bg-transparent border-0">
            <div class="card-body">
              <div id="mouseCarousel" class="carousel-fade" data-bs-ride="carousel">
                <div class="carousel-inner">
                  <% mouse.forEach((mouse, index) => { %>
                    <% var mouseImages = mouse.product_image.split(','); %>
                    <% mouseImages.forEach((image, imgIndex) => { %>
                      <div class="carousel-item <%= imgIndex === 0 && index === 0 ? 'active' : '' %>" data-bs-interval="2000">
                        <img src="/static/images/category/product/<%= image.trim() %>" alt="<%= mouse.product_title %>" class="img-fluid d-block w-100">
                      </div>
                    <% }); %>
                  <% }); %>
                </div>
              </div>
              <div class="card-footer h6 fw-bold text-light mt-3">MOUSE</div>
            </div>
          </div>
        </div>
        
        <!-- Headphones Tab -->
        <div class="col-4 p-5" id="nav-contact-tab" data-bs-toggle="tab" data-bs-target="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false">
          <div class="card bg-transparent border-0">
            <div class="card-body">
              <div id="headphoneCarousel" class="carousel-fade" data-bs-ride="carousel">
                <div class="carousel-inner">
                  <% headphone.forEach((headphone, index) => { %>
                    <% var headphoneImages = headphone.product_image.split(','); %>
                    <% headphoneImages.forEach((image, imgIndex) => { %>
                      <div class="carousel-item <%= imgIndex === 0 && index === 0 ? 'active' : '' %>" data-bs-interval="2000">
                        <img src="/static/images/category/product/<%= image.trim() %>" alt="<%= headphone.product_title %>" class="img-fluid d-block w-100">
                      </div>
                    <% }); %>
                  <% }); %>
                </div>
              </div>
              <div class="card-footer h6 fw-bold text-light mt-3">HEADPHONES</div>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    
